  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "cpy": "^8.1.1",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "ignore": "^5.1.8",
    "ink": "^3.0.8",
    "ink-big-text": "^1.2.0",
    "ink-spinner": "^4.0.1",
    "is-ssh": "^1.3.2",
    "meow": "^5.0.0",
    "pkg-dir": "^5.0.0",
    "react": "^17.0.1",
    "simple-git": "^3.5.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/ink-big-text": "^1.2.0",
    "@types/is-ssh": "^1.3.0",
    "@types/meow": "^5.0.0",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^5.0.4",
    "pinst": "^2.1.1",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "dev": "npm run build:watch",
    "prebuild": "npm run clean",
